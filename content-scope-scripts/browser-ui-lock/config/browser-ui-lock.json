{
    "readme": "Config for browser UI lock integration tests.",
    "version": 1,
    "unprotectedTemporary": [],
    "features": {
        "webCompat": {
            "state": "enabled",
            "exceptions": [],
            "settings": {
                "browserUiLock": {
                    "state": "enabled",
                    "overscrollBehavior": "enabled",
                    "overflow": "enabled",
                    "overflowClip": "disabled",
                    "observeMutations": "enabled",
                    "postLoadDelayMs": 300
                }
            }
        }
    }
}
